@article{Benmamoun:1999b,	Author = {Benmamoun, Elabbas},	Date-Modified = {2007-02-13 17:33:06 -0500},	Journal = {Linguistic Inquiry},	Local-Url = {file://localhost/Users/kyle/Documents/Bookends/Attachments/*Linguistic%20Inquiry/30.4Benmamoun.pdf},	Number = {4},	Pages = {621--642},	Title = {The Syntax of Quantifiers and Quantifier Float},	Volume = {30},	Year = {1999},	Abstract = {The Arabic quantifier kull displays a Q_NP and NP_Q alternation. Shlonsky (1991) argues that in both patterns Q heads a QP projection with the NP as a complement that may undergo movement to [Spec, QP] or beyond to yield the NP_Q pattern and Q-float structures. On the contrary, I argue on the basis of evidence from reconstruction, Case, and agreement that the two patterrns are radically different. In the Q_NP pattern Q is indeed the head of a QP projection that contains the NP. In the NP_Q pattern, however, Q heads a QP adjunct that modifies the NP and in some cases the VP.}}
