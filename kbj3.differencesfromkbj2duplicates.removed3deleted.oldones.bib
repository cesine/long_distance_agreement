@article{Bahan:2000,	Author = {Bahan, Benjamin and Kegl, Judy and Lee, Robert G. and MacLaughlin, Dawn and Neidle, Carol},	Date-Modified = {2007-03-16 16:56:10 -0400},	Journal = {Linguistic Inquiry},	Keywords = {library greement ign language},	Local-Url = {file://localhost/Users/kyle/Documents/Bookends/Attachments/*Linguistic%20Inquiry/31.1Bahan_etal.pdf},	Number = {1},	Pages = {1--27},	Title = {The licensing of null arguments in {A}merican {S}ign {L}anguage},	Volume = {31},	Year = {2000},	Abstract = {The distribution of null arguments across languages has been accounted for in terms of two distinct strategies: licensing by agreement and licensing by topic. Lillo-Martin (1986, 1991) claims that American Sign Language (ASL) exploits both strategies for licensing null arguments, depending on the morphological characteristics of the verb. Here we show that this is incorrect. Once the nonmanual correlates of agreement features (comparable to the nonmanual expressions of other syntactic features) in ASL are recognized, it becomes apparent that null argument in this language are systematically licnesed by an expression of syntactic agreement.}}
