@article{Munn:1999a,	Author = {Munn, Alan},	Date-Modified = {2007-02-13 17:33:48 -0500},	Journal = {Linguistic Inquiry},	Keywords = {coordination},	Local-Url = {file://localhost/Users/kyle/Documents/Bookends/Attachments/*Linguistic%20Inquiry/30.4Munn.pdf},	Number = {4},	Pages = {643--668},	Title = {First Conjunct Agreement: Against a Clausal Analysis},	Volume = {30},	Year = {1999},	Abstract = {Aoun, Benmamoun, and Sportiche (1994) propose that first conjunct agreement in Arabic is derived from clausal conjunction. This article shows that the clausal account of first conjunct agreement is empirically inadequate because it fails to distinguish clearly between syntactic and semantic agreement. It argues that an analysis of coordination as adjunction proposed in Munn 1992, 1993 accounts both for the Arabic facts and for the fact that first conjunct agreement is dependent on head government. It also shows that there are asymmetries between governed agreement and spcifier-head agreement that are independent of coordination, lending support to a unified analysis of first conjunct agreement in terms of government or its equivalent.}}
