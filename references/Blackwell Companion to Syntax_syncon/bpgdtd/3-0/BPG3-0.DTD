     ==================================================== -->


<!--MathML parameter entities for attributes-->
<!ENTITY % a-globl      'class CDATA #IMPLIED
                         style CDATA #IMPLIED
                         id    ID    #IMPLIED
                         other CDATA #IMPLIED'>
<!ENTITY % a-open       'open CDATA #IMPLIED'>
<!ENTITY % a-close      'close CDATA #IMPLIED'>
<!ENTITY % a-separ      'separat CDATA #IMPLIED'>
<!ELEMENT tex      - -  CDATA>


<!ELEMENT math      - -  (%maths; | tex)+   -(math)>
<!ELEMENT mrow      - -  (%maths;)>
<!ATTLIST mrow      %a-globl;>

<!ELEMENT mfrac          - -  (mrow, mrow)>
<!--mfrac: 1st mrow is numerator; 2nd is denominator-->

<!ELEMENT msqrt          - -  (%maths;)>
<!ELEMENT mroot          - -  (mrow, mrow)>
<!--mroot: 1st mrow is base; 2nd is index-->

<!ELEMENT msubsup        - -  (mrow, mrow, mrow)>
<!--msubsup: 1st mrow is base; 2nd is subscript; 3rd is superscript-->

<!ELEMENT mmultscr  - -  (mrow, (mrow, mrow)+, (mprescr, (mrow, mrow)+)?)>
<!--msubsup: 1st mrow is base; (2nd is subscript; 3rd is superscript). 
    (mrow, mrow) following mprescr are (subscript, superscript)-->

<!ELEMENT munder         - -  (%maths;)>
<!ELEMENT mover          - -  (%maths;)>
<!ELEMENT mundrovr     - -  (mrow, mrow, mrow)>
<!--mundrovr: 1st mrow is base; 2nd is underscript; 3rd is overscript-->

<!ELEMENT mtable         - -  (mtr+)>
<!ELEMENT mtext         - -  (%maths;)>
<!ELEMENT mtr            - -  (mtd+)>
<!ELEMENT mtd            - -  (%maths;)>
<!ELEMENT mfenced        - -  (%maths;)>
<!ATTLIST mfenced   %a-open;
                    %a-close;
                    %a-separ;
                    %a-globl;>
<!ELEMENT mprescr   - o EMPTY>
<!ATTLIST mprescr   %a-globl;>

