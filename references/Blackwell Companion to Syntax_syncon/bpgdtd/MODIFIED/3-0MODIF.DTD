			colsep %yesorno;       #IMPLIED
			rowsep %yesorno;       #IMPLIED
			orient (port |land)    #IMPLIED
			pgwide %yesorno;       #IMPLIED
			type   (inline |icon |hscroll) "inline" >

<!ELEMENT tbltl  - -  (p+) >

<!ELEMENT tgroup  - -  (colspec*, thead?, tfoot?, tbody+, tfoot?)>
